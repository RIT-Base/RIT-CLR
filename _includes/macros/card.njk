<a href="{{ item.url }}" class="resource-card">
    <div class="card-content">
        <h4>{{ item.data.title }}</h4>
        <p>{{ item.data.description }}</p>
        <div class="card-footer">
            <span class="tag">{{ item.data.level }}</span>
            <span class="tag">{{ item.data.type }}</span>
        </div>
    </div>
</a>